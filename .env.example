# Environment template - copy to .env and fill in values
AIRFLOW_UID=50000

# Airflow webserver credentials
_AIRFLOW_WWW_USER_USERNAME=airflow
_AIRFLOW_WWW_USER_PASSWORD=airflow

# HDR API Key - get your own from https://hdrdata.org/
HDR_API_KEY=your_api_key_here

# SMTP Configuration for email alerts (optional)

SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_STARTTLS=True
SMTP_SSL=False
SMTP_MAIL_FROM=your-email@gmail.com
